{
	"name": "wxa",
	"version": "1.0.0",
	"devDependencies": {
		"@babel/core": "^7.1.6",
		"@babel/preset-env": "^7.1.6",
		"@vuepress/plugin-back-to-top": "^1.0.0-alpha.23",
		"@vuepress/plugin-google-analytics": "^1.0.0-alpha.23",
		"@vuepress/plugin-medium-zoom": "^1.0.0-alpha.0",
		"@vuepress/plugin-notification": "^1.0.0-alpha.23",
		"@vuepress/plugin-pwa": "^1.0.0-alpha.23",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^23.6.0",
		"commitizen": "^3.0.4",
		"cz-conventional-changelog": "^2.1.0",
		"husky": "^0.14.3",
		"jest": "^23.6.0",
		"lerna": "^2.9.0",
		"lint-staged": "^7.3.0",
		"regenerator-runtime": "^0.13.1",
		"vuepress": "^1.0.0-alpha.23"
	},
	"scripts": {
		"publish-next": "lerna publish --npm-tag=next --canary beta --ignore @wxa/plugin-dependencies-analysis",
		"commit": "git-cz",
		"test": "jest"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		]
	},
	"sideEffects": false,
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-proposal-decorators": "^7.1.6"
	}
